{
  "name": "ide-perl6",
  "main": "./lib/main",
  "version": "0.2.0",
  "description": "Perl 6 language support for Atom-IDE",
  "keywords": [
    "langserver",
    "perl6"
  ],
  "repository": "https://github.com/azawawi/ide-perl6",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.9.6",
    "atom-package-deps": "^4.6.2"
  },
  "package-deps": [
    "atom-ide-ui",
    "language-perl6"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
